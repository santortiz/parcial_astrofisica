plot "parabolico20.dat" u 2:3 w l lc 'red' title '20 clase', "parabolico100.dat" u 2:3 w l lc 'blue' title '100', "parabolico1e6.dat" u 2:3 w l lc 'black' title '1e6', "leapfrog20.dat" u 2:3 w l lc 'red' dt 5 title '20 leapfrog', "leapfrog100.dat" u 2:3 w l lc 'blue' dt 5 title '100', "leapfrog1000000.dat" u 2:3 w l lc 'black' dt 5 title '1e6'